<template>
  <Modal v-model="show" :mask-closable="!1" className="yellow" width="300">
      <p slot="header">
      <Icon type="ios-add-circle-outline" ></Icon>
      <span>签收</span>
    </p>
    <p>您确定进行签收吗</p>
    <div slot="footer">
      <Button type="success" @click="onOk" >是</Button>
      <Button type="error" @click="onCancel" >否</Button>
    </div>
  </Modal>
</template>
<script>
export default {
  data() {
    return {
      id: false,
      show: false,
      type:""
    };
  },
  mounted() {
  },
  methods: {
    open(id,type) {
      this.show = true;
      this.id  = id;
      this.type = type;
    },
    onOk() {
     if(this.type == "20"){
        this.$store.commit({
					type: "notice/noticeError",
					title: "存在二次分派配送单！不可签收"
				});
        this.show = false;
      }else{
        this.$store.dispatch({ type: "delStock/signData",id:this.id });
        this.show = false;
      }
      
    },
    onCancel() {
     this.show = false;
    }
  }
};
</script>

