.loginBox {
  width: 100%;
  height: 100%;
  background-size: cover;
  position: relative;
  background-image: url("./login.jpg");
  background-image: url("./bac.jpg");
  background-repeat: no-repeat;
  display: table;
}

.loginBox * {
  -webkit-transition: all .4s ease-in-out;
  transition: all .4s ease-in-out;
}

.loginBox .container {
  display: table-cell;
  vertical-align: middle;
}

.loginBox .container .login {
  margin: auto;
  width: 360px;
}

.loginBox .container .login .logeImage {
  margin: 0 auto;
  width: 100%;
  margin-bottom: 25px;
}

.loginBox .container .login .header {
  text-align: center;
  margin: 0 auto;
  font-size: 20px;
  font-weight: bold;
  color: #fff;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
}

.loginBox .container .login .bodyer {
  width: 360px;
  margin-top: 20px;
  padding: 30px;
  border-radius: 5px;
  -webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.2);
          box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.2);
  background-color: rgba(255, 255, 255, 0.2);
  border: 1px solid rgba(255, 255, 255, 0.3);
  border-width: 1px 0 0 1px;
}

.loginBox .container .login .bodyer:hover {
  -webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
          box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
  background-color: rgba(255, 255, 255, 0.3);
}

.loginBox .container .login .bodyer input {
  border: 1px solid #9bb8d6;
}

.loginBox .container .login .bodyer .ivu-form-item-content {
  white-space: nowrap;
}

.loginBox .container .login .bodyer .ivu-form-item-error .ivu-input {
  border: 1px solid #ed4014;
}

.loginBox .container .login .bodyer .form-title {
  font-weight: 300;
  margin-bottom: 25px;
  color: #eee;
  font-size: 24.5px;
}

.loginBox .container .login .bodyer .codingText {
  width: 153px;
}

.loginBox .container .login .bodyer .codingImgBox {
  height: 30px;
  display: inline-block;
  vertical-align: middle;
  border-radius: 3px;
  border: 1px solid #fff;
  background-color: #fff;
  overflow: hidden;
}

.loginBox .container .login .bodyer .codingImgBox img {
  max-width: 100%;
  max-height: 100%;
}

.loginBox .container .login .bodyer .ivu-input-icon {
  font-weight: bold;
}

.loginBox .container .login .bodyer button {
  padding: 4px 6px 7px;
  -webkit-box-shadow: 1px 1px 1px rgba(255, 255, 255, 0.5) inset;
          box-shadow: 1px 1px 1px rgba(255, 255, 255, 0.5) inset;
  background-color: #2d8cf0;
}

.loginBox .container .login .bodyer button:hover {
  -webkit-box-shadow: -1px -1px 1px rgba(0, 0, 0, 0.2) inset;
          box-shadow: -1px -1px 1px rgba(0, 0, 0, 0.2) inset;
}

.loginBox .container .login .bodyer button .loginBtn {
  height: 16px;
  font-size: 16px;
}

.loginBox .container .login .bodyer button .loginBtn .n {
  opacity: 0;
}

.loginBox .container .login .footer {
  clear: both;
  margin-top: 20px;
  text-align: center;
  color: #fff;
  font-size: 14px;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
}
